<div class="container" style="width:60%;">
	<form class="form-horizontal" role="form" action='' method="POST">

		<div class="form-group">
			<label class="control-label" for="name">Name</label>
			<input readonly="true" type="text" id="name" name="name"
				placeholder="" class="form-control" />
			<p class="help-block" />
		</div>

		<div class="form-group">
			<!-- E-mail -->
			<label class="control-label" for="email">E-mail
			</label>
			<input readonly="true" type="text" id="email" name="email"
				placeholder="" class="form-control" />
		</div>

		<div class="form-group">
			<label class="control-label" for="contactNo">Contact No
			</label>
			<input readonly="true" type="text" id="contactNo" name="contactNo"
				placeholder="" class="form-control" />
		</div>

		<div class="form-group">
			<label class="control-label" for="drivingLicenseNo">Driving License</label>
			<input readonly="true" type="text" id="drivingLicenseNo" name="drivingLicenseNo"
				placeholder="" class="form-control" />
			<p class="help-block"></p>
		</div>


		<div class="form-group">
			<label class="control-label" for="gender">Gender</label>

			<input readonly="true" type='text' name='gender' id='gender'
				placeholder="" class="form-control" />
			<p class="help-block"></p>
		</div>

		<div class="form-group">
			<label class="control-label" for="birthday">Birth Date</label>

			<input readonly="true" type='text' name='birthDay' id='birthDay'
				placeholder="" class="form-control" />
			<p class="help-block"></p>
		</div>


		<div class="form-group">
			<label class="control-label" for="address">Street Address
			</label>

			<input readonly="true" type='text' name='address' id='address'
				placeholder="" class="form-control" />
			<p class="help-block"></p>
		</div>

		<div class="form-group">
			<label class="control-label" for="city">City</label>

			<input readonly="true" type='text' name='city' id='city'
				placeholder="" class="form-control" />
			<p class="help-block"></p>
		</div>


		<div class="form-group">
			<label class="control-label" for="state">State</label>

			<input readonly="true" type='text' name='state' id='state'
				placeholder="" class="form-control" />
			<p class="help-block"></p>
		</div>

		<div class="form-group">
			<label class="control-label" for="pin">Pin</label>

			<input readonly="true" type='text' name='pin' id='pin'
				placeholder="" class="form-control" />
			<p class="help-block"></p>
		</div>


		<div class="form-group">
			<label class="control-label" for="country">Country</label>

			<input readonly="true" type='text' name='country' id='country'
				placeholder="" class="form-control" />
			<p class="help-block"></p>
		</div>
	</form>
</div>


