<div style="position:relative">
	<div id="messegePanel" class="alert alert-danger" style="display:none">
	</div>

	<strong>Vehicle:</strong>
	<a id="vehicleId" href="javascript:void(0);"></a>

	<br />

	<div class="toolBarContent">
		<span style="margin-right:5px;">
			<span>From Date: </span>
			<input id="fromDate" type="text" />
		</span>
		<span style="margin-right:5px;">
			<span>To Date: </span>
			<input id="toDate" type="text" />
		</span>
		<span>
			<span>Start Time: </span>
			<input id="startTime" type="text" />
		</span>
		<span>
			<span>Vacant Seats: </span>
			<select id="totalSeats">
				<option value="1">1</option>
				<option value="2">2</option>
				<option value="3">3</option>
				<option value="4">4</option>
			</select>
		</span>
		<span>
			<span>Bucks/Km </span>
			<select id="bucksPerKm">
				<option value="1">1</option>
				<option value="2">2</option>
				<option value="3">3</option>
				<option value="4">4</option>
				<option value="5">5</option>
				<option value="6">6</option>
				<option value="7">7</option>
				<option value="8">8</option>
				<option value="9">9</option>
				<option value="10">10</option>
				<option value="11">11</option>
				<option value="12">12</option>
				<option value="13">13</option>
				<option value="14">14</option>
			</select>
		</span>
		<span id="savePool">
			<input id="savePoolButton" type="button" value="Save" />
		</span>
	</div>

	<div id="users" style="display:none" class="container bootstrap snippet">
		<div class="row">
			<div class="col-lg-12">
				<div class="main-box no-header clearfix">
					<div class="main-box-body clearfix">
						<div class="table-responsive">
							<table class="table user-list">
								<thead>
									<tr>
										<th>
											<span>User</span>
										</th>
										<th class="text-center">
											<span>Name</span>
										</th>
										<th class="text-center">
											<span>Cell no.</span>
										</th>
									</tr>
								</thead>
								<tbody>
								</tbody>
							</table>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class="mapContent">
		<input id="pac-input" class="controls" type="text" placeholder="Search Box" />
		<div id="map-canvas" style="width:100%; height:100%"></div>
	</div>
</div>